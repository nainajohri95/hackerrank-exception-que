import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

class Solution 
{
    void func1()
    {
        try
        {
            Scanner sc = new Scanner(System.in);
            int var1 = sc.nextInt();
            int var2 = sc.nextInt();
        
            int res = var1/var2;
            System.out.println(res); 
        }
        
        catch(Exception e)
        {
             System.out.println(e.toString());
        }
    }
    public static void main(String args[])
    {
        Solution obj = new Solution();
        obj.func1();
    }
}
